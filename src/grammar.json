{
  "name": "lean",
  "rules": {
    "source_file": {
      "type": "REPEAT",
      "content": {
        "type": "SYMBOL",
        "name": "_command"
      }
    },
    "_command": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "_hash_command"
        },
        {
          "type": "SYMBOL",
          "name": "namespace_definition"
        },
        {
          "type": "SYMBOL",
          "name": "section_definition"
        },
        {
          "type": "SYMBOL",
          "name": "function_definition"
        },
        {
          "type": "SYMBOL",
          "name": "theorem_definition"
        }
      ]
    },
    "_hash_command": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "eval_command"
        }
      ]
    },
    "eval_command": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "#eval"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        }
      ]
    },
    "namespace_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "namespace"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_command"
          }
        },
        {
          "type": "STRING",
          "value": "end"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "section_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "section"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "_command"
          }
        },
        {
          "type": "STRING",
          "value": "end"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "function_definition": {
      "type": "CHOICE",
      "members": []
    },
    "theorem_definition": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "theorem"
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "_expression"
        },
        {
          "type": "STRING",
          "value": ":="
        },
        {
          "type": "SYMBOL",
          "name": "identifier"
        }
      ]
    },
    "_expression": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "identifier"
        },
        {
          "type": "SYMBOL",
          "name": "number"
        }
      ]
    },
    "identifier": {
      "type": "PATTERN",
      "value": "[A-za-z]+"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
